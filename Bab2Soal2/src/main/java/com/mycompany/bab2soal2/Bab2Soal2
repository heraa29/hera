package com.mycompany.bab2soal2;

/**
 *
 * @author Lenovo
 */
public class Bab2Soal2 {
   // Variabel statis
    static String MakananFavorit;
    static String MinumanFavorit;

    public static void main(String[] args) {
  // Mengisi nilai variabel
        MakananFavorit = "Ayam Geprek";
        MinumanFavorit = "Jus Jambu";

        // Menampilkan output
        System.out.println("Makanan Favorit saya : " + MakananFavorit);
        System.out.println("Minuman Favorit saya : " + MinumanFavorit);
    }
}