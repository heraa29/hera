package com.mycompany.bab9soal3;

/**
 *
 * @author Lenovo
 */
public class Bab9Soal3 {

    public static void main(String[] args) {
        int i = 1;
        while (i < 6) { // Perulangan selama i kurang dari 6
            System.out.println(i); // Menampilkan nilai i
            i++; // Menambahkan 1 ke i setelah setiap iterasi
        }
    }
}